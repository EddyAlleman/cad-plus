<?xml version="1.0" encoding="utf-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
    <Fragment>
        <DirectoryRef Id="INSTALLFOLDER">
          <Component Id="xport" Guid="*">
            <File Id="xport.exe" KeyPath="yes" Source="$(var.SourceOutDir)\xport.exe" />
          </Component>
          <Component Id="xbatch" Guid="*">
            <File Id="xbatch.exe" KeyPath="yes" Source="$(var.SourceOutDir)\xbatch.exe" />
          </Component>
        </DirectoryRef>
    </Fragment>
    <Fragment>
        <ComponentGroup Id="MainFilesRegGroup">
            <ComponentRef Id="xport" />
            <ComponentRef Id="xbatch" />
        </ComponentGroup>
    </Fragment>
</Wix>